{
  "expo": {
    "name": "InstantRecipes",
    "slug": "InstantRecipes",
    "scheme": "instantrecipes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoapp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f7f7f8"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Necesitamos acceso a la cámara para que puedas tomar fotos.",
        "NSPhotoLibraryUsageDescription": "Necesitamos acceso a la galería para que puedas seleccionar fotos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoapp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aleu79.InstantRecipes",
      "permissions": [
        "camera",
        "read_external_storage",
        "write_external_storage"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "redirectUri": "https://instantrecipes.guticao.net.ar/api",
      "eas": {
        "projectId": "da9e0e26-efe3-4133-ba81-5f2616872ced"
      }
    },
    "plugins": [
      [
        "expo-file-system",
        {
          "photosPermission": "La aplicación necesita acceso a tus fotos para compartir recetas."
        }
      ]
    ]
  }
}
